(run){
  QCUT:=20.0
  DCUT:=0.3
  NUM_JETS:=0
  LEPTON_PID:=11
  NEUTRINO_PID:=12

  EVENTS       = 100
  NUM_ACCURACY = 1.e-10
  OUTPUT       = 2
  ANALYSIS     = 1
  EVENT_MODE   = HepMC
  RANDOM_SEED  = 15532 27624
}(run)

(beam){
  BEAM_1        = 2212
  BEAM_ENERGY_1 = 3500.
  BEAM_2        = 2212
  BEAM_ENERGY_2 = 3500.
}(beam)

(me){
  ME_SIGNAL_GENERATOR   = Comix
  EVENT_GENERATION_MODE = Unweighted
  ME_QED                = On        ! Use "On" for YFS
}(me)

(mi){
  MI_HANDLER = Amisic
}(mi)

(shower){
  CSS_EW_MODE = 0  ! 0/1 turns the EW PS off/on
}(shower)

(processes){
  Process           93 93 ->  LEPTON_PID -NEUTRINO_PID 22 93{NUM_JETS}
  Order_EW          3
  CKKW              sqr(QCUT/E_CMS)|DCUT
  Print_Graphs      1
  Integration_Error 0.02
  End process

  Process           93 93 -> -LEPTON_PID  NEUTRINO_PID 22 93{NUM_JETS}
  Order_EW          3
  CKKW              sqr(QCUT/E_CMS)|DCUT
  Print_Graphs      1
  Integration_Error 0.02
  End process
}(processes)

(selector){
  ET     22       5.0  7000.0
  PT     90       5.0  7000.0
  DeltaR 90  22   0.3    10.0
}(selector)
